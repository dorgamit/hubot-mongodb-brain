{
  "name": "hubot-mongodb-brain",
  "description": "Persist hubot's brain to MongoDB instead of Redis.",
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "contributors": [
    "Lucas Hrabovsky <lucas@mongodb.com>"
  ],
  "version": "1.0.0",
  "scripts": {
    "fmt": "mongodb-js-fmt",
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "npm run check && npm test",
    "postinstall": "./node_modules/.bin/hubot-script-installer",
    "postuninstall": "./node_modules/.bin/hubot-script-installer --uninstall"
  },
  "homepage": "http://github.com/mongodb-js/hubot-mongodb-brain",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/hubot-mongodb-brain.git"
  },
  "dependencies": {
    "hubot-script-installer": "0.0.0",
    "mongodb": "^2.0.52",
    "mongodb-redact": "0.0.1",
    "mongodb-url": "^1.0.3"
  },
  "devDependencies": {
    "eslint-config-mongodb-js": "^1.0.6",
    "mocha": "^2.3.4",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "mongodb-runner": "^3.1.14",
    "pre-commit": "^1.1.2"
  },
  "license": "Apache-2.0",
  "precommit": [
    "check"
  ]
}
